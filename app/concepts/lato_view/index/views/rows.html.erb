<% if(@elements) %>
  <tbody>
    <% @elements.each do |element| %>
    <tr>
      <% @attributes.each_with_index do |attribute, index| %>
        <% value = element.send(attribute) %>
        <% if(!!value == value) %>
          <td data-label="<%= @head[index] %>">
            <% if value %>
              <div class="general-status">
                <!-- <div class="status icon-status true">
                  <svg xmlns="http://www.w3.org/2000/svg" width="150" height="200" viewBox="0 0 150 200"><path d="M143.1 83.7c-1.8 1.8-4.1 3-7.1 3-2.4 0-4.7-1.2-7.1-3L84.6 39.3v143.1c0 5.3-4.1 10.1-10.1 10.1-5.9 0-10.1-4.1-10.1-10.1V39.3L20.2 83.7c-4.1 4.1-10.1 4.1-14.2 0S1.9 73.6 6 69.5L67.5 8.6c4.1-4.1 10.1-4.1 13.6 0l61.5 60.9c4.1 4.1 4.1 10.6.5 14.2z"/></svg>
                </div> -->
                <div class="status round-status true">
                  
                </div>
              </div>
            <% else %>
              <div class="general-status">
                <!-- <div class="status icon-status false">
                  <svg xmlns="http://www.w3.org/2000/svg" width="150" height="200" viewBox="0 0 150 200"><path d="M143.3 130.5l-60.8 60.8c-1.8 1.8-4.3 3.1-6.8 3.1s-4.9-1.2-6.8-3.1L8.3 130.5c-3.7-3.7-3.7-9.8 0-14.1s9.8-3.7 14.1 0l44.2 44.2V18.1c0-5.5 4.3-9.8 9.8-9.8s9.8 4.3 9.8 9.8v142.4l44.2-44.2c3.7-3.7 9.8-3.7 14.1 0 3.1 3.7 3.1 9.9-1.2 14.2z"/></svg>
                </div> -->
                <div class="status round-status false">
                  
                </div>
              </div>
            <% end %>
          </td>
        <% else %>
          <td data-label="<%= @head[index] %>"><%= value %></td>
        <% end %>
      <% end %>
      <% if @show_link || @edit_link || @delete_link %>
        <td class="actions">
          <% if(@show_link) %>
            <%= link_to VIEW_LANG['index']['show'], self.show_link(element.id), class: 'lato-button' %>
          <% end %>
          <% if(@edit_link) %>
            <%= link_to VIEW_LANG['index']['edit'], self.edit_link(element.id), class: 'lato-button button-yellow' %>
          <% end %>
          <% if(@delete_link) %>
            <%= link_to VIEW_LANG['index']['delete'], self.delete_link(element.id), method: :delete, remote: @remote_delete, class: 'lato-button button-red' %>
          <% end %>
        </td>
      <% end %>
    </tr>
  <% end %>
  </tbody>
<% end %>
